<div id="<%= dom_id contact %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= contact.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">First name:</strong>
    <%= contact.first_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Last name:</strong>
    <%= contact.last_name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Phone number:</strong>
    <%= contact.phone_number %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= contact.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Address:</strong>
    <%= contact.address %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Category:</strong>
    <%= contact.category %>
  </div>
</div>
